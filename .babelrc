{
    "plugins": [
        "react-require",
        "babel-plugin-transform-es2015-modules-commonjs",
        "@babel/plugin-transform-react-jsx"
    ],
    "env": {
        "production": {
            "presets": [
                ["@babel/preset-env", {
                    "targets": {
                        "browsers": ["last 2 versions", "safari >= 7"]
                    }
                }]
            ]
        }
    }
}
